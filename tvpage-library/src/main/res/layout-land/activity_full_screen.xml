<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relFulllscreen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/framesparent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000">

        <com.tvpage.lib.view.TvPagePlayer xmlns:VideoView="http://schemas.android.com/apk/res-auto"
            android:id="@+id/tvPagePlayer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            VideoView:useDefaultControls="false" />
    </FrameLayout>
</RelativeLayout>
